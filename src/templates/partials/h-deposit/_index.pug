mixin h-deposit-4
  .h-deposit&attributes(attributes)
    .h-deposit__titles
      p.h-deposit__text Choose a payment method.
      p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Funds are credited instantly and without commission
    .h-deposit__list
      .h-deposit__payement.h-deposit__payement--active
        .h-deposit__service
          img(src="https://via.placeholder.com/100x40/#6C90FC").img
        .h-deposit__wrap
          .h-deposit__table
            .h-deposit__tr
              .h-deposit__td
                p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Min
                p.h-deposit__text 10$
              .h-deposit__td
                p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Max
                p.h-deposit__text 100$
              .h-deposit__td
                p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Сomission
                p.h-deposit__text 0%
              .h-deposit__td
                p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey In processing
                p.h-deposit__text instantly
            .h-deposit__tr
              .h-deposit__td
                button.h-deposit__pay Deposit
      -var n = 0
      while (n < 5)
        .h-deposit__payement(data=n++)
          .h-deposit__service
            img(src="https://via.placeholder.com/100x40/#6C90FC").img
          .h-deposit__wrap
            .h-deposit__table
              .h-deposit__tr
                .h-deposit__td
                  p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Min
                  p.h-deposit__text 10$
                .h-deposit__td
                  p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Max
                  p.h-deposit__text 100$
                .h-deposit__td
                  p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey Сomission
                  p.h-deposit__text 0%
                .h-deposit__td
                  p.h-deposit__text.h-deposit__text--sm.h-deposit__text--grey In processing
                  p.h-deposit__text instantly
              .h-deposit__tr
                .h-deposit__td
                  button.h-deposit__pay Deposit
